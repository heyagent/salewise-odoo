<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="salewise_social_action_social_post" model="ir.actions.act_window">
            <field name="name">Social Posts</field>
            <field name="res_model">social.post</field>
            <field name="view_mode">kanban,calendar,form</field>
            <field name="original_action" ref="social.action_social_post" />
            <field name="original_view_mode">kanban,calendar,list,form,pivot</field>
            <field name="context">{'search_default_filter_my_posts':1, 'salewise_mode': True}</field>
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_social_post_kanban')}), (0, 0, {'view_mode': 'calendar', 'view_id': ref('salewise_views.salewise_social_post_calendar')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_social_post_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_social_post_search" />
        </record>
        <record id="salewise_social_action_social_stream_post" model="ir.actions.act_window">
            <field name="name">Feed</field>
            <field name="res_model">social.stream.post</field>
            <field name="view_mode">kanban</field>
            <field name="original_action" ref="social.action_social_stream_post" />
            <field name="original_view_mode">kanban</field>
            <field name="context">{'search_default_group_by_stream': 1, 'search_default_search_my_streams': 1, 'salewise_mode': True}</field>
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_social_stream_post_kanban')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_social_stream_post_search" />
        </record>
