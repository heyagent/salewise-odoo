            <field name="view_mode">kanban,form</field>
            <field name="original_action" ref="social.action_view_utm_campaigns" />
            <field name="original_view_mode">kanban,list,form</field>
            <field name="domain">[('is_auto_campaign', '=', False)]</field>
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_utm_campaign_kanban')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_utm_campaign_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_utm_campaign_search" />
