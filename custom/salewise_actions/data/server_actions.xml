<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- CRM Server Actions -->
    <record id="salewise_crm_action_your_pipeline" model="ir.actions.server">
        <field name="name">Crm: My Pipeline</field>
        <field name="model_id" ref="crm.model_crm_team"/>
        <field name="state">code</field>
        <field name="code">action = model.salewise_action_your_pipeline()</field>
            <field name="is_saas">True</field>
            <field name="is_system">True</field>
        <field name="original_action" ref="crm.action_your_pipeline"/>
    </record>
    <record id="salewise_crm_action_opportunity_forecast" model="ir.actions.server">
        <field name="name">Crm: Forecast</field>
        <field name="model_id" ref="crm.model_crm_team"/>
        <field name="state">code</field>
        <field name="code">action = model.salewise_action_opportunity_forecast()</field>
            <field name="is_saas">True</field>
            <field name="is_system">True</field>
        <field name="original_action" ref="crm.action_opportunity_forecast"/>
    </record>

    </data>
</odoo>