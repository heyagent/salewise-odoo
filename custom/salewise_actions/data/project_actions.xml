<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="salewise_project_open_view_project_all" model="ir.actions.act_window">
            <field name="name">Projects</field>
            <field name="res_model">project.project</field>
            <field name="view_mode">kanban,form</field>
            <field name="original_action" ref="project.open_view_project_all" />
            <field name="original_view_mode">kanban,list,form</field>
            <field name="context">{
                                'default_composition_mode': 'mass_mail',
                        }, 'salewise_mode': True}</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="search_view_id" ref="salewise_views.salewise_project_project_search" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_project_project_kanban')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_project_project_form')})]" />
        </record>
        <record id="salewise_project_action_view_all_task" model="ir.actions.act_window">
            <field name="name">All Tasks</field>
            <field name="res_model">project.task</field>
            <field name="view_mode">list,form,calendar</field>
            <field name="original_action" ref="project.action_view_all_task" />
            <field name="original_view_mode">list,kanban,form,calendar,pivot,graph,activity</field>
            <field name="domain">[('project_id', '=', active_id), ('display_in_project', '=', True)]</field>
            <field name="context">{
                                'active_model': 'project.project',
                                'default_project_id': active_id,
                                'show_project_update': True,
                                'search_default_open_tasks': 1,
                        }, 'salewise_mode': True}</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="search_view_id" ref="salewise_views.salewise_project_task_search" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'list', 'view_id': ref('salewise_views.salewise_project_task_list')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_project_task_form')}), (0, 0, {'view_mode': 'calendar', 'view_id': ref('salewise_views.salewise_project_task_calendar')})]" />
        </record>
        <record id="salewise_project_project_project_stage_configure" model="ir.actions.act_window">
            <field name="name">Project Stages</field>
            <field name="res_model">project.project.stage</field>
            <field name="view_mode">kanban,form</field>
            <field name="original_action" ref="project.project_project_stage_configure" />
            <field name="original_view_mode">list,kanban,form</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_project_project_stage_kanban')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_project_project_stage_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_project_project_stage_search" />
        </record>
        <record id="salewise_project_open_task_type_form" model="ir.actions.act_window">
            <field name="name">Task Stages</field>
            <field name="res_model">project.task.type</field>
            <field name="view_mode">kanban,form</field>
            <field name="original_action" ref="project.open_task_type_form" />
            <field name="original_view_mode">list,kanban,form</field>
            <field name="domain">[('user_id', '=', False)]</field>
            <field name="context">{"default_user_id": false, 'salewise_mode': True}</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_project_task_type_kanban')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_project_task_type_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_project_task_type_search" />
        </record>
        <record id="salewise_documents_document_action" model="ir.actions.act_window">
            <field name="name">Documents</field>
            <field name="res_model">documents.document</field>
            <field name="view_mode">kanban</field>
            <field name="original_action" ref="documents.document_action" />
            <field name="original_view_mode">kanban,list,activity</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_documents_document_kanban')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_documents_document_search" />
        </record>
        <record id="salewise_calendar_action_calendar_event" model="ir.actions.act_window">
            <field name="name">Meetings</field>
            <field name="res_model">calendar.event</field>
            <field name="view_mode">calendar,list,form</field>
            <field name="original_action" ref="calendar.action_calendar_event" />
            <field name="original_view_mode">calendar,list,form</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="search_view_id" ref="salewise_views.salewise_calendar_event_search" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'calendar', 'view_id': ref('salewise_views.salewise_calendar_event_calendar')}), (0, 0, {'view_mode': 'list', 'view_id': ref('salewise_views.salewise_calendar_event_list')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_calendar_event_form')})]" />
        </record>
        <record id="salewise_calendar_action_calendar_alarm" model="ir.actions.act_window">
            <field name="name">Calendar Alarm</field>
            <field name="res_model">calendar.alarm</field>
            <field name="view_mode">list,form</field>
            <field name="original_action" ref="calendar.action_calendar_alarm" />
            <field name="original_view_mode">list,form</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'list', 'view_id': ref('salewise_views.salewise_calendar_alarm_list')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_calendar_alarm_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_calendar_alarm_search" />
        </record>
        <record id="salewise_appointment_appointment_type_action" model="ir.actions.act_window">
            <field name="name">Appointments</field>
            <field name="res_model">appointment.type</field>
            <field name="view_mode">kanban,form</field>
            <field name="original_action" ref="appointment.appointment_type_action" />
            <field name="original_view_mode">kanban,list,form</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'kanban', 'view_id': ref('salewise_views.salewise_appointment_type_kanban')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_appointment_type_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_appointment_type_search" />
        </record>
        <record id="salewise_appointment_appointment_invite_action" model="ir.actions.act_window">
            <field name="name">Share Links</field>
            <field name="res_model">appointment.invite</field>
            <field name="view_mode">list,form</field>
            <field name="original_action" ref="appointment.appointment_invite_action" />
            <field name="original_view_mode">list</field>
            <field name="context">{'search_default_my_book_urls': 1, 'salewise_mode': True}</field>
            <field name="is_saas">True</field>
            <field name="plan_id" ref="salewise_plans.plan_professional" />
            <field name="view_ids" eval="[(5, 0, 0), (0, 0, {'view_mode': 'list', 'view_id': ref('salewise_views.salewise_appointment_invite_list')}), (0, 0, {'view_mode': 'form', 'view_id': ref('salewise_views.salewise_appointment_invite_form')})]" />
            <field name="search_view_id" ref="salewise_views.salewise_appointment_invite_search" />
        </record>
    </data>
</odoo>